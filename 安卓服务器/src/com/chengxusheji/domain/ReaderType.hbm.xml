<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="ReaderType" table="ReaderType">

          <!-- 主键读者类型编号 -->
          <id name="readerTypeId" column="readerTypeId">
            <generator class="native"/>
          </id>

          <!-- 简单字符串属性: 读者类型 -->
          <property
            name="readerTypeName"
            column="readerTypeName"
            type="string"
            length="20"
          />

          <!-- 简单整型属性: 可借阅数目 -->
          <property
            name="number"
            column="number"
            type="int"

          />


		 
		
	</class>
	
</hibernate-mapping>
 
